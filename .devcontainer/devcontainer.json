{
    "name": "Yocto Development Container",
    "build": {
      "dockerfile": "Dockerfile"
    },
    "customizations":{
        "settings": {
            "terminal.integrated.shell.linux": "/bin/bash"
          },
          "extensions": [
            "ms-vscode.cpptools",
            "ms-vscode.cmake-tools",
            "ms-python.python"
          ]
    },
    "remoteUser": "yocto",
    "forwardPorts": [22]
  }
  